{
    "name": "com.quizvideo.app",
    "displayName": "Quizvideo",
    "version": "1.0.0",
    "description": "A sample Apache Cordova application that responds to the deviceready event.",
    "main": "index.js",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --open",
        "build": "ng build",
        "test": "npm run lint && ng test --configuration=test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "watch": "ng test --configuration=test --browsers ChromeHeadless --watch",
        "build:prod": "ng build --prod --build-optimizer --vendor-chunk --common-chunk",
        "build:staging": "ng build --configuration=staging --build-optimizer --vendor-chunk --common-chunk",
        "build:dev": "ng build --configuration=dev",
        "clean": "rimraf ./dist/",
        "server": "cd dist && http-server",
        "staging": "npm run clean && npm run build:staging",
        "prod": "npm run clean && npm run build:prod && npm run server",
        "ci": "npm run clean && npm run prettier:ci && ng lint && ng test --configuration=test --browsers ChromeTravisCi && ng e2e && npm run build:prod -- --deploy-url /qv/ --base-href /qv",
        "release": "standard-version && git push --follow-tags origin master",
        "prettier": "prettier {src,e2e}/**/*.{ts,json,md,scss} --write",
        "prettier:ci": "prettier {src,e2e}/**/*.{ts,json,md,scss} --list-different",
        "analyze": "npm run clean && npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/stats.json",
        "cordova": "ng build --prod --base-href ./ --output-path ./www/"
    },
    "author": "Apache Cordova Team",
    "license": "Apache-2.0",
    "dependencies": {
        "@angular/animations": "^6.1.4",
        "@angular/cdk": "^6.4.6",
        "@angular/common": "^6.1.4",
        "@angular/compiler": "^6.1.4",
        "@angular/core": "^6.1.4",
        "@angular/forms": "^6.1.4",
        "@angular/http": "^6.1.4",
        "@angular/material": "^6.4.3",
        "@angular/platform-browser": "^6.1.4",
        "@angular/platform-browser-dynamic": "^6.1.4",
        "@angular/router": "^6.1.4",
        "@auth0/angular-jwt": "^2.0.0",
        "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
        "@ngrx/effects": "^6.1.0",
        "@ngrx/store": "^6.1.0",
        "@ngrx/store-devtools": "^6.1.0",
        "@ngx-translate/core": "^10.0.2",
        "@ngx-translate/http-loader": "^3.0.1",
        "@types/socket.io-client": "^1.4.32",
        "ajv": "^6.5.3",
        "angular5-social-login": "^1.0.9",
        "angularfire2": "^5.1.1",
        "browser-detect": "^0.2.27",
        "clipboard": "^2.0.1",
        "cordova-android": "^7.0.0",
        "cordova-ios": "^4.5.5",
        "cordova-plugin-device": "^2.0.2",
        "cordova-plugin-firebase": "^2.0.5",
        "cordova-plugin-inappbrowser": "^3.0.0",
        "cordova-plugin-preventsleep": "^1.1.1",
        "cordova-plugin-safariviewcontroller": "^1.5.4",
        "cordova-plugin-splashscreen": "^5.0.2",
        "cordova-plugin-statusbar": "^2.4.2",
        "cordova-plugin-streaming-media": "git+https://github.com/nchutchind/cordova-plugin-streaming-media.git",
        "cordova-plugin-whitelist": "^1.3.3",
        "cordova-support-android-plugin": "^1.0.1",
        "cordova-support-google-services": "^1.2.1",
        "cordova-universal-links-plugin": "^1.2.1",
        "core-js": "^2.5.4",
        "firebase": "^5.7.0",
        "hammerjs": "^2.0.8",
        "hls.js": "^0.11.0",
        "html2canvas": "1.0.0-alpha.12",
        "jquery": "^1.12.4",
